sample presentational component:
const ComponentName = props => <div onClick={props.handleClick}>I am just happy.</div>

componentDidMount() {
  fetch(`https://api.spoonacular.com/recipes/random?number=5&apiKey=${apiKey}`)
    .then(resp =>resp.json())
    .then(data => {
      this.setState({
        recipes: data.recipes
      })
    })
}
